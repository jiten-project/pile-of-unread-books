{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "cache": {
        "key": "dev-v1"
      }
    },
    "development:device": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "cache": {
        "key": "dev-device-v1"
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "key": "preview-v1"
      }
    },
    "production": {
      "autoIncrement": true,
      "credentialsSource": "remote",
      "cache": {
        "key": "prod-v1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "language": "ja-JP"
      }
    }
  }
}
